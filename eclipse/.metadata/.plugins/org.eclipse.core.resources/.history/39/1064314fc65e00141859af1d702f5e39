package com.amazingthings.core.items;

import com.amazingthings.core.AmazingCore;

import net.minecraft.item.Item;

public class IngotCopper extends Item {

	public IngotCopper() {
		super();
		this.setTextureName("atcore:ingotCopper");
		this.setUnlocalizedName("ingotCopper");
		this.setCreativeTab(AmazingCore.tabCore);
	}
}
